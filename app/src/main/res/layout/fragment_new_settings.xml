<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.yogis.atemsaa_fragments.fragments.NewSettingsFragment">

        <include layout="@layout/content_scrolling_settings" />


        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fl_plc_mms"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="260dp"
            android:src="@mipmap/ic_transformador"
            android:elevation="6dp"
            app:pressedTranslationZ="12dp"
            app:backgroundTint="@color/colorGreen"
            android:visibility="invisible"
            />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/txt_plc_mms"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PLC_MMS"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="70dp"
            android:layout_marginBottom="270dp"
            android:visibility="invisible"
            />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fl_plc_tu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="180dp"
            android:src="@mipmap/ic_medidor"
            android:elevation="6dp"
            app:pressedTranslationZ="12dp"
            app:backgroundTint="@color/colorGreen"
            android:visibility="invisible"
            />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/txt_plc_tu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PLC_TU"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="70dp"
            android:layout_marginBottom="190dp"
            android:visibility="invisible"
            />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fl_plc_mc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="100dp"
            android:src="@mipmap/ic_armario"
            android:elevation="6dp"
            app:pressedTranslationZ="12dp"
            app:backgroundTint="@color/colorGreen"
            android:visibility="invisible"
            />


        <android.support.v7.widget.AppCompatButton
            android:id="@+id/txt_plc_mc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PLC_TU"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="70dp"
            android:layout_marginBottom="110dp"
            android:visibility="invisible"
            />
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fl_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:src="@mipmap/ic_mas"
            android:elevation="6dp"
            app:pressedTranslationZ="6dp"
            app:backgroundTint="@color/colorGreen"

            />

        </android.support.design.widget.CoordinatorLayout>




